<TABLE class = "list">
  <CAPTION>Курорт</CAPTION>
    <THEAD>
      <TR>
        <TH>1</TH>
        <TH>2</TH>
        <TH>3</TH>
        <TH>4</TH>


      </TR>
    </THEAD>
    <TBODY>
      <% @items.each do |p| %>
      <TR>
        <TD><%= p.name %></TD>
        <TD><%= p.state %></TD>
        <TD><%= p.tip %></TD>
        <TD><%= p.rank %></TD>

        <%= inline_menu(p) %>
      </TR>
      <% end %>
    </TBODY>
</TABLE>
